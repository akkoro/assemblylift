module {{snake_case name}}_container_registry {
    source = "./services/{{name}}/infra/{{container_registry.provider.name}}"

    project_name = "{{project_name}}"
    service_name = "{{name}}"

    providers = {
        {{container_registry.platform.name}} = {{container_registry.platform.name}}.{{container_registry.platform.id}}
    }
}
